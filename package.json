{
  "name": "linkhub2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "start": "node .output/server/index.mjs",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "lint:fix": "biome lint --fix",
    "check": "biome check",
    "cf-typegen": "wrangler types --env-interface Env",
    "deploy:prod": "wrangler deploy --env production",
    "deploy:staging": "wrangler deploy --env staging",
    "db-generate": "drizzle-kit generate",
    "db:local:migrate": "wrangler d1 migrations apply DB --local",
    "db:local:query": "wrangler d1 execute DB --local --command",
    "db:local:studio": "drizzle-kit studio",
    "db:prod:migrate": "wrangler d1 migrations apply DB --env production --remote",
    "db:prod:query": "wrangler d1 execute DB --env production --remote --command",
    "db:staging:migrate": "wrangler d1 migrations apply DB --env staging --remote",
    "db:staging:query": "wrangler d1 execute DB --env staging --remote --command",
    "db:local:reset": "rm -f .wrangler/state/v3/d1/miniflare-D1DatabaseObject/* && pnpm db:local:migrate",
    "db:test": "tsx scripts/test-db.ts",
    "generate-wrangler": "tsx scripts/generate-wrangler.ts",
    "clean": "rm -rf .output dist node_modules/.vite",
    "clean:all": "rm -rf .output dist node_modules/.vite .wrangler",
    "reset": "pnpm clean:all && rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "db:local:execute": "wrangler d1 execute DB --local --file",
    "db:prod:execute": "wrangler d1 execute DB --env production --remote --file",
    "db:staging:execute": "wrangler d1 execute DB --env staging --remote --file"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-oauth/google": "^0.12.2",
    "@rive-app/react-canvas": "^4.23.3",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.2.2",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.131.5",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.131.7",
    "@tanstack/router-plugin": "^1.121.2",
    "@tanstack/zod-adapter": "^1.131.49",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "gsap": "^3.13.0",
    "jose": "^6.1.0",
    "lucide-react": "^0.544.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-share-kit": "^1.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "drizzle-kit": "^0.31.4",
    "jsdom": "^26.0.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4",
    "wrangler": "^4.40.3"
  }
}